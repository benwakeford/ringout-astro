{
  "name": "rooklanearts-org-uk-ringout-astro",
  "type": "module",
  "version": "0.0.1",
  "prettier": {
    "astroAllowShorthand": true,
    "overrides": [
      {
        "files": "*.astro",
        "options": {
          "parser": "astro"
        }
      }
    ],
    "pluginSearchDirs": [
      "."
    ],
    "plugins": [
      "prettier-plugin-astro"
    ]
  },
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro"
  },
  "dependencies": {
    "@astrojs/react": "^3.0.2",
    "@astrojs/tailwind": "^5.0.0",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "astro": "^3.1.4",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "tailwindcss": "^3.0.24"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.16",
    "cssnano": "^6.0.1",
    "postcss-import": "^15.1.0",
    "prettier": "^3.0.3",
    "prettier-config-standard": "^7.0.0",
    "prettier-plugin-astro": "^0.12.0",
    "sass": "^1.68.0"
  }
}
